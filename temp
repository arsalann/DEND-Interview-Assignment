curl --location --request GET "https://climate.weather.gc.ca/climate_data/bulk_data_e.html?format=csv&stationID=14&Year=2020&Month=11&Day=1&timeframe=2&submit=Download+Data" > 14.csv
curl --location --request GET "https://climate.weather.gc.ca/climate_data/bulk_data_e.html?format=csv&stationID=0&Year=2020&Month=11&Day=1&timeframe=2&submit=Download+Data" > 0.csv



